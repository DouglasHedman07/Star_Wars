<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Pantalla1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/botonSpaceships"
        android:layout_width="160dp"
        android:layout_height="230dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="180dp"
        android:background="@drawable/spaceships"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/botonPeople"
        android:layout_width="160dp"
        android:layout_height="230dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/people"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/botonVehicles"
        android:layout_width="160dp"
        android:layout_height="230dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="70dp"
        android:background="@drawable/vehicles"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/botonPlanets"
        android:layout_width="160dp"
        android:layout_height="230dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="70dp"
        android:background="@drawable/planets"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Switch
        android:id="@+id/reproductor1"
        android:layout_width="96dp"
        android:layout_height="41dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="16dp"
        android:text="Switch"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/numeroPantalla1"
        android:layout_width="51dp"
        android:layout_height="63dp"
        android:layout_marginStart="180dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/boton2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
